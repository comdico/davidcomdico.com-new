  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-handlebars": "^1.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "handlebars": "^4.7.8",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11"
  }
}
